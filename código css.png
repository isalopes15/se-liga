*{
   margin: 0;
   padding: 0;
   box-sizing: border-box;
}

body{
    font-family: Arial, sans-serif;
    line-heigh: 1.6;
    color: #333;
    background-color: #f7f7f7;
}

header{
    background: #000;
    color: #fff;
    padding: 20xp 10%;
    display: flex;
    justify-content: space-between;
    aling-items: center;
}

header .logo{
    font-size: 1.5em;
    font-weight:bold;
}

header nav ul {
    list-style: none;
    display: flex;
}

header nav ul li{
    margin: 0 15px;
}

header nav ul li a {
    color: #fff;
    text-decoration: none;
    font-size: 1em;
    transition: color 0.3s;
}
header nav ul li a {
    color: #fdd835;
}

.full-width-image img {
    width: 100%;
    height: 450px;
    object-fit: cover;
    display: block;
}

.dark-box{
   background: #000;
   color: #fff;
   text-align: center;
   padding: 30px;
   border-radius: 8px;
   margin: 20px 0;
   box-shadow: 0 2px 5px rgda(0, 0, 0, 0.3);
}

.dark-box h2 {
   color: #fff;
   margin-bottom: 20xp;
}

.dark-box p {
   color: #fff;
   font-size: 1em;
   margin-bottom: 20xp;
}

.dark-box .section-image {
  width: 100%;
  max-width: 600px;
  height: auto;
  margin: 20px auto;
  border-redius: 8px;
  box-shadow: 0 2px 5px rgba(0 ,0 ,0 ,0.2);
  }


























